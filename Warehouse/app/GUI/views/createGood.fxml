<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="800.0" prefWidth="1050.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="app.gui.Controllers.CreateGood">
   <children>
      <Label layoutX="29.0" layoutY="26.0" text="Създаване на Стока">
         <font>
            <Font name="System Bold" size="36.0" />
         </font>
      </Label>
      <VBox layoutX="31.0" layoutY="97.0" prefHeight="245.0" prefWidth="300.0" spacing="15.0">
         <children>
            <TextField fx:id="good" promptText="Име на новата стока">
               <font>
                  <Font size="16.0" />
               </font>
            </TextField>
            <TextField fx:id="price" promptText="Цена на новата стока (лв.)">
               <font>
                  <Font size="16.0" />
               </font>
            </TextField>
            <TextField fx:id="quantity" promptText="Количество">
               <font>
                  <Font size="16.0" />
               </font>
            </TextField>
            <TextField fx:id="minQuantity" promptText="Минимално количество">
               <font>
                  <Font size="16.0" />
               </font>
            </TextField>
            <Button fx:id="button" mnemonicParsing="false" onAction="#createButtonClicked" prefHeight="25.0" prefWidth="302.0" text="Създай">
               <font>
                  <Font size="16.0" />
               </font>
            </Button>
         </children>
      </VBox>
   </children>
</AnchorPane>
