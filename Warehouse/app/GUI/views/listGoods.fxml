<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="800" prefWidth="1050.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="app.gui.Controllers.ListGoods">
    <children>
        <VBox prefHeight="800.0" prefWidth="1050.0" spacing="10.0">
            <children>
                <Label prefHeight="53.0" prefWidth="385.0" text="Списък с Наличности">
                    <font>
                        <Font name="System Bold" size="36.0" />
                    </font>
                </Label>
                <TableView fx:id="table" prefHeight="716.0" prefWidth="1030.0">
                    <columns>
                        <TableColumn fx:id="goodColumn" text="Стока" />
                        <TableColumn fx:id="priceColumn" text="Цена (лв.)" />
                        <TableColumn fx:id="quantityColumn" text="Количество" />
                        <TableColumn fx:id="minQuantityColumn" text="Мин. Количество" />
                    </columns>
                    <columnResizePolicy>
                        <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
                    </columnResizePolicy>
                </TableView>
            </children>
            <padding>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
            </padding>
        </VBox>
    </children>
</AnchorPane>
